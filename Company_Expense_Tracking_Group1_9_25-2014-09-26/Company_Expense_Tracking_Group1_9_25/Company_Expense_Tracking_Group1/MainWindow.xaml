<Window x:Name="LogIn_Window" x:Class="Company_Expense_Tracking_Group1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="345.156" Width="492.301">
    <Grid>
        <Button x:Name="Sign_In_Button" Content="Sign In" HorizontalAlignment="Left" Height="49" Margin="136,223,0,0" VerticalAlignment="Top" Width="210" FontSize="20" FontWeight="Bold" Click="Sign_In_Button_Click"/>
        <TextBox x:Name="Password_TextBox" HorizontalAlignment="Left" Height="50" Margin="136,157,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="210" FontSize="20" TextChanged="Password_TextBox_TextChanged"/>
        <ComboBox x:Name="UserName_ComboBox" HorizontalAlignment="Left" Height="53" Margin="136,87,0,0" VerticalAlignment="Top" Width="210" SelectionChanged="UserName_ComboBox_SelectionChanged"/>
        <Label x:Name="UserLogIn_Label" Content="User Name" HorizontalAlignment="Left" Height="44" Margin="139,38,0,0" VerticalAlignment="Top" Width="242" FontSize="20"/>
        <Label x:Name="Date_Time" Content="date/time" HorizontalAlignment="Left" Margin="162,10,0,0" VerticalAlignment="Top" Width="157"/>

    </Grid>
</Window>
